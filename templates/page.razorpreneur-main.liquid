  span.rzp-head {
    font-size: 48px;
  }
  .s3-slide {
    text-align: center;
    font-size: 20px;
    font-family: 'Roboto';
    font-weight: 400
  }
  .s4-main-section {
    display: block;
    max-width: 100%;
    width: 800px;
    margin: 0 auto;
    background-color: #ffecae;
    border-radius: 30px;
    box-shadow: 2px 3px 1px #000;
  }
  .s4-slide {
    font-size: 19px;
    text-align: center;
    line-height: 22px;
    padding: 25px 15px;
    font-family: Roboto, sans-serif;
    font-style: italic;
    font-weight: 400;
  }
  .s4-slide h3 {
    font-style: normal
  }
  img.arrow {
    position: absolute;
    top: 70px;
    width: 40px;
    height: 40px;
    transition: 0.3s;
  }
  img.arrow:hover {
    transform: scale(1.1)
  }
  .left {
    left: 200px;
  }
  .right {
    right: 200px;
  }
  .s4-main {
    padding-bottom: 15px;
    position: relative
  }
  .section-2 {
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 20px;
  }
  .login {
    width: 40%;

  }
  .login h2 {
    font-size: 24px;
    font-weight: 700;
    font-family: Roboto, sans-serif;
    text-transform: uppercase;
    text-align: center;
  }
  @media(max-width:450px) {
    video.desktopp {
      display: none
    }
    video.mobilee {
      display: block;
    }
    .s3-slide {
      margin: 0 12px;
    }
  }
